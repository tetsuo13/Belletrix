@using Belletrix.Entity.Model;

@model IEnumerable<StudyAbroadModel>

@{ 
    Html.ViewData.Add(new KeyValuePair<string, object>("ShowStudent", false));
}

<div>
    <br />
    @Html.Partial("StudyAbroadList.Partial", Model, Html.ViewData)
</div>

<div class="row">
    <div class="col-lg-12">
        <a href="@Url.Action("Add", "Experience", new { studentId = ViewBag.StudentId })" class="btn btn-default" role="button">Add New Experience</a>
    </div>
</div>
